capture_directory: ~
fixed_frame: map
target_frame: robot_base
fix_orientation: false
rotate_90: false
enable_antialiasing: true
show_displays: true
show_status_bar: true
show_capture_tools: true
window_width: 1920
window_height: 1056
view_scale: 0.3746763
offset_x: 0
offset_y: 0
use_latest_transforms: true
background: "#a0a0a4"
displays:
  - type: mapviz_plugins/odometry
    name: filtered_gps
    config:
      visible: true
      collapsed: false
      topic: /odometry/filtered_gps
      color: "#ffff00"
      draw_style: points
      position_tolerance: 0
      buffer_size: 0
      show_laps: true
      show_covariance: true
      static_arrow_sizes: true
      arrow_size: 218
      show_timestamps: 0
  - type: mapviz_plugins/odometry
    name: filtered_global
    config:
      visible: true
      collapsed: false
      topic: /odometry/filtered_global
      color: "#ffaaff"
      draw_style: points
      position_tolerance: 0
      buffer_size: 10
      show_laps: true
      show_covariance: true
      static_arrow_sizes: true
      arrow_size: 1
      show_timestamps: 0
  - type: mapviz_plugins/odometry
    name: filtered_local
    config:
      visible: true
      collapsed: false
      topic: /odometry/filtered_global
      color: "#a0fffc"
      draw_style: lines
      position_tolerance: 0
      buffer_size: 50
      show_laps: true
      show_covariance: true
      static_arrow_sizes: false
      arrow_size: 14
      show_timestamps: 0
  - type: mapviz_plugins/grid
    name: odom
    config:
      visible: true
      collapsed: true
      color: "#ffffff"
      alpha: 0.1
      frame: map
      x: -10
      y: -10
      size: 1
      rows: 20
      columns: 20
  - type: mapviz_plugins/grid
    name: map
    config:
      visible: true
      collapsed: true
      color: "#b0fff6"
      alpha: 0.3
      frame: odom
      x: -5
      y: -5
      size: 1
      rows: 10
      columns: 10
  - type: mapviz_plugins/attitude_indicator
    name: new display
    config:
      visible: true
      collapsed: false
      topic: /imu/data_fixed
      x: 0
      y: 0
      width: 100
      height: 100