capture_directory: ~
fixed_frame: map
target_frame: map
fix_orientation: false
rotate_90: false
enable_antialiasing: true
show_displays: true
show_status_bar: true
show_capture_tools: true
window_width: 1900
window_height: 1007
view_scale: 0.1815811
offset_x: -63.76411
offset_y: 0.3146758
use_latest_transforms: true
background: "#a0a0a4"
image_transport: raw
displays:
  - type: mapviz_plugins/tile_map
    name: google_maps
    config:
      visible: true
      collapsed: false
      custom_sources:
        - base_url: http://localhost:8080/wmts/gm_layer/gm_grid/{level}/{x}/{y}.png
          max_zoom: 19
          name: google_maps
          type: wmts
        - base_url: "http://c.tile.stamen.com/watercolor/${z}/${x}/${y}.jpg\t"
          max_zoom: 19
          name: stae
          type: wmts
        - base_url: "https://localhost:8080/wmts/gm_layer/gm_grid/{level}/{x}/{y}.png\n"
          max_zoom: 19
          name: tesy
          type: wmts
      bing_api_key: ""
      source: google_maps
  - type: mapviz_plugins/odometry
    name: filtered_gps
    config:
      visible: true
      collapsed: true
      topic: /odometry/filtered_gps
      color: "#ffff00"
      draw_style: points
      position_tolerance: 0
      buffer_size: 0
      show_laps: true
      show_covariance: true
      static_arrow_sizes: true
      arrow_size: 218
      show_timestamps: 0
  - type: mapviz_plugins/odometry
    name: filtered_global
    config:
      visible: true
      collapsed: true
      topic: /odometry/filtered_global
      color: "#ffaaff"
      draw_style: points
      position_tolerance: 0
      buffer_size: 0
      show_laps: true
      show_covariance: true
      static_arrow_sizes: true
      arrow_size: 1
      show_timestamps: 0
  - type: mapviz_plugins/odometry
    name: filtered_local
    config:
      visible: true
      collapsed: true
      topic: /odometry/filtered_global
      color: "#a0fffc"
      draw_style: lines
      position_tolerance: 0
      buffer_size: 0
      show_laps: true
      show_covariance: true
      static_arrow_sizes: false
      arrow_size: 5
      show_timestamps: 0
  - type: mapviz_plugins/tf_frame
    name: new display
    config:
      visible: true
      collapsed: true
      frame: back_left_wheel
      color: "#000000"
      draw_style: arrows
      position_tolerance: 0
      buffer_size: 1
      static_arrow_sizes: true
      arrow_size: 38
  - type: mapviz_plugins/coordinate_picker
    name: new display
    config:
      visible: true
      collapsed: true
      frame: origin
      copy: true
  - type: mapviz_plugins/attitude_indicator
    name: new display
    config:
      visible: true
      collapsed: true
      topic: /imu/data_fixed
      x: 0
      y: 0
      width: 100
      height: 100
  - type: mapviz_plugins/grid
    name: map
    config:
      visible: true
      collapsed: true
      color: "#b0fff6"
      alpha: 0.3
      frame: odom
      x: -5
      y: -5
      size: 1
      rows: 10
      columns: 10
  - type: mapviz_plugins/grid
    name: odom
    config:
      visible: true
      collapsed: true
      color: "#ffffff"
      alpha: 0.1
      frame: map
      x: -10
      y: -10
      size: 0
      rows: 20
      columns: 20
  - type: mapviz_plugins/tf_frame
    name: new display
    config:
      visible: true
      collapsed: true
      frame: utm
      color: "#ffffff"
      draw_style: arrows
      position_tolerance: 0
      buffer_size: 0
      static_arrow_sizes: true
      arrow_size: 66
  - type: mapviz_plugins/move_base
    name: new display
    config:
      visible: true
      collapsed: false